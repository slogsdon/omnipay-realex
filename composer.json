{
    "name": "digitickets/omnipay-realex",
    "type": "library",
    "description": "Realex driver with 3D Secure & refund support for Omnipay payment processing library",
    "keywords": [
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "purchase",
        "realex"
    ],
    "homepage": "https://github.com/digitickets/omnipay-realex",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Coates",
            "email": "andycoatz@gmail.com"
        }
    ],
    "autoload": {
        "psr-0": { "Omnipay\\Realex\\" : "src/" }
    },
    "autoload-dev": {
        "psr-0": { "Omnipay\\Realex\\Tests\\" : "tests/" }
    },
    "require": {
        "omnipay/common": "~2.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "~1.5",
        "omnipay/tests": "^2.1"
    },
    "scripts": {
        "lint": "phpcs --standard=PSR2 src tests",
        "test": "@composer lint && phpunit --coverage-text"
    }
}
